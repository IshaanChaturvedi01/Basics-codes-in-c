#include<stdio.h>
#include <string.h>
struct Student{
    char name[50];
    char major[50];
    int age;
    double gpa;
};
int main()
{
  struct Student student1;
      student1.age=32;
      student1.gpa=3.2;
      strcpy(student1.name,"Jim");                           
      strcpy(student1.major,"Buisness");

      printf("%f\n",student1.gpa);

struct Student student2;
      student2.age=32;
      student2.gpa=3.2;
      strcpy(student2.name,"tim");                          
      strcpy(student2.major,"Buisness");


      printf("%f",student2.gpa);
    return 0;
}
